.filter
	%ul
		%li= link_to "Manage Users", user_settings_path
		%li.active= link_to "Managae Holidays", holiday_settings_path
		%li= link_to "Manage Templates", ""
		.clear
#userForm
	- if @day_sequence.new_record?
		= form_for @day_sequence, :url => day_sequences_path do |f|
			- render :partial => "settings/day_sequences/form", :locals => { :f => f }
	- else
		= form_for @day_sequence, :url => day_sequence_path(@day_sequence) do |f|
			- render :partial => "settings/day_sequences/form", :locals => { :f => f }
	.clear

#holidays